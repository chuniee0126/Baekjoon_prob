    "def result(N):\n",
    "    if (N%5) == 0:\n",
    "        print N/5 + (N%5)/3\n",
    "    elif (N%5) == 1 or (N%5) == 4 :\n",
    "        print (N-5)/5 + (N%5 + 5)/3\n",
    "    else:\n",
    "        if N%3 == 0:\n",
    "            print N/3\n",
    "        else :\n",
    "            print -1\n",
    "\n",
    "result(input())"
   ]
